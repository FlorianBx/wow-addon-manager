{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "WoW Addon Manager",
  "version": "0.1.0",
  "identifier": "fr.skyliss.wow-addon-manager",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "WoW Addon Manager",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IENDRjM3QkQwMTM0QUNCNUYKUldSZnkwb1QwSHZ6eklnMzBUMVhyVitCTllqNEFzdzg3SVlkTlFRZU8vSEFYczhGUVcvcXY2SlcK",
      "endpoints": [
        "https://github.com/florianbx/wow-addon-manager/releases/latest/download/latest.json"
      ]
    }
  }
}
